class Solution {
    public static int findKthPositive(int[] arr, int k) {
        Map<Integer, Integer> map = new HashMap<>();  
        int count = 0;
        for(int i = 0;i<arr.length;i++)
            map.put(arr[i],1);
        int j=1;
        while(count!=k)
        {
            if(map.containsKey(j))
                j++;
            else{
                count++;
                j++;
            }
        }
        return j-1;
    }
}
